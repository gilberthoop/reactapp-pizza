{
  "name": "willpizzeria",
  "author": "William Go",
  "version": "0.0.0",
  "homepage": "http://gilberthoop.github.io/willpizzeria",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client  && npm run build --prefix client"
  },
  "engines": {
    "node": "13.7.x",
    "npm": "6.13.x"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie-parser": "~1.4.4",
    "create-react-app": "^3.3.1",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-validator": "^6.4.1",
    "frontend": "^2.0.0-alpha.4",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.8.10",
    "morgan": "~1.9.1",
    "react-router-dom": "^5.1.2",
    "sweetalert": "^2.1.2"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}